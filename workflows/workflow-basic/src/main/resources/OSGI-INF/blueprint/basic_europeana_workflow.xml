<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:bp="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:shell="http://karaf.apache.org/xmlns/shell/v1.0.0"
           xmlns:cm="http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.0.0">

    <bp:reference id="registry" interface="eu.europeana.uim.api.Registry"/>

    <bean id="iiwClient" class="eu.europeana.uim.europeanaspecific.workflows.InitialIngestionWorkflow">
    </bean>
    
    <bean id="basicClient" class="eu.europeana.uim.europeanaspecific.workflows.TestWorkflow">
    </bean>

    <service id="initialIngestionWorkflow" ref="iiwClient" interface="eu.europeana.uim.workflow.Workflow"/>
    <service id="testWorkflow" ref="basicClient" interface="eu.europeana.uim.workflow.Workflow"/>

</blueprint>